{
  "name": "posh-weather",
  "version": "0.1.0",
  "private": true,
  "description": "HTTPS=true npm run start",
  "homepage": "https://posh-weather.netlify.com",
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.4.1",
    "axios": "^0.18.0",
    "node-localstorage": "^1.3.1",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "npm": "^6.4.1",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-icons": "^3.0.5",
    "react-lottie": "^1.2.3",
    "react-scripts": "1.1.4",
    "react-spring": "^5.6.6",
    "styled-components": "^3.4.2"
  },
  "scripts": {
    "build-css": "npm run node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
